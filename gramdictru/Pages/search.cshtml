@page "{term?}"
@model searchModel

<h2>Searching</h2>

<script>
    window["term"] = "@Html.Raw(Model.Term)";
    const urlParams = new URLSearchParams(window.location.search);
    window["symbol"] = urlParams.get('symbol') || "";
    window["lists"] = urlParams.get('list') || "";
    window["additionals"] = urlParams.get('additional') || "";
    window["index"] = urlParams.get('index') || "";
    window["stress"] = urlParams.get('stress') || "";
    window["circle"] = urlParams.get('circle') || "";
    window["para"] = urlParams.get('para') || "";
</script>